function tTest(sample, mu, alpha, type)
    m = mean(sample);
    n = size(sample, 2);
    
    s = 0;
    
    for i = 1:n
        s = s + (sample(i) - m)^2;
    end
    
    s = s/(n-1);
    
    t = (m - mu)/(sqrt(s/n));
    
    disp("sample mean = " + m)
    disp("sample variance = " + s)
    disp("observed t value = " + t)
end

